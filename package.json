{
  "name": "kin-chrome-extension",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-ext": "webpack --config webpack/webpack.prod.js --env.base=./webpack.ext.js --progress",
    "build-post-install": "webpack --config webpack/webpack.prod.js --env.base=./webpack.post_install.js --progress",
    "watch-ext": "webpack --config  webpack/webpack.dev.js --env.base=./webpack.ext.js --watch",
    "watch-post-install": "webpack --config webpack/webpack.dev.js --env.base=./webpack.post_install.js --watch",
    "build": "npm-run-all build-ext build-post-install",
    "watch": "npm run watch-ext & npm run watch-post-install",
    "clean": "rimraf dist"
  },
  "author": "Kin Ecosystem",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "0.0.86",
    "babel-eslint": "^10.0.3",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.2.0",
    "eslint": "^6.2.2",
    "eslint-plugin-vue": "^5.2.3",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.3.1",
    "style-loader": "^1.0.0",
    "ts-loader": "^5.4.5",
    "typescript": "~3.4.5",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "~4.17.2",
    "webpack-cli": "~3.1.0",
    "webpack-merge": "~4.2.1"
  },
  "dependencies": {
    "@kinecosystem/kin-sdk-js": "^0.1.11",
    "@kinecosystem/kin-sdk-js-keystore-providers": "^1.0.24",
    "lodash": "^4.17.15",
    "marked": "^0.7.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.2"
  }
}
